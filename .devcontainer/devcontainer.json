{
    "name": "All patterns",
    "image": "mcr.microsoft.com/devcontainers/dotnet:6.0",
    "features": {
        "ghcr.io/devcontainers/features/azure-cli:1": {
            "installBicep": true,
            "extensions": "cosmosdb-preview"
        }
    },
    "containerEnv": {
        "ASPNETCORE_ENVIRONMENT": "Development"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-dotnettools.csharp",
                "ms-azuretools.vscode-bicep"
            ]
        }
    }
}